<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean id="actor-info" class="com.stackroute.domain.Actor">
<property name="name" value="Prabhas"></property>
<property name="gender" value="male"></property>
<property name="age" value="35"></property>
</bean>
<bean id="movie-info" class="com.stackroute.domain.Movie">
<property name="actor" ref="actor-info"></property> 
<property name="movieName" value="BahuBali"></property>
</bean>

</beans>
